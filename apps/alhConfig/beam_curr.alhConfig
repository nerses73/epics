CHANNEL  BEAM_CURRENT                 scaler_calc1                
$COMMAND  medm -x -attach -cmap -macro "sig=scaler_calc1" aiaocalc_alarm.adl >> /dev/null 
$ALARMCOUNTFILTER  10 120
